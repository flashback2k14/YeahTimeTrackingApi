{
  "exclude": ["./coverage", "./dist", "jest.config.js", "src/**/*.test.ts"],
  "compilerOptions": {
    "rootDirs": ["src", "prisma"],
    "outDir": "./dist",
    "lib": ["esnext"],
    "target": "es2016",
    "module": "CommonJS",
    "moduleResolution": "node",
    "strict": true,
    "checkJs": false,
    "sourceMap": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true
  }
}
