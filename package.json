{
  "name": "yeahtimetrackingapi",
  "description": "Yeah! Time tracking API",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flashback2k14/YeahTimeTrackingApi.git"
  },
  "keywords": [],
  "author": "flashback2k14",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flashback2k14/YeahTimeTrackingApi/issues"
  },
  "homepage": "https://github.com/flashback2k14/YeahTimeTrackingApi#readme",
  "main": "src/index.ts",
  "scripts": {
    "init": "npm run dirs && npm run docs && npm run prisma:ts:to:db",
    "init:with:data": "npm run init && npm run prisma:seed",
    "dev": "NODE_ENV=development concurrently \"npx tsc --watch\" \"nodemon -q dist/src/index.js\"",
    "start": "NODE_ENV=production node --trace-deprecation dist/src/index.js",
    "build": "npx tsc",
    "test": "NODE_ENV=testing jest",
    "test:cov": "NODE_ENV=testing jest --coverage",
    "format": "prettier --write .",
    "clean:all": "rm -rf dist/ db/ static/ coverage/",
    "dirs": "node scripts/init.js",
    "docs": "node scripts/swagger.js",
    "prisma:db:to:ts": "prisma db pull",
    "prisma:ts:to:db": "prisma db push",
    "prisma:seed": "prisma db seed",
    "prisma:reset": "prisma migrate reset --force",
    "prisma:ui": "prisma studio",
    "prisma": "prisma"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "crypto-js": "^4.1.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "swagger-ui-express": "^4.6.2"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.14.5",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^7.6.0",
    "jest": "^29.4.3",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "prisma": "^4.11.0",
    "supertest": "^6.3.3",
    "swagger-autogen": "^2.23.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}