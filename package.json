{
  "name": "yeahtimetrackingapi",
  "description": "Yeah! Time tracking API",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flashback2k14/YeahTimeTrackingApi.git"
  },
  "keywords": [],
  "author": "flashback2k14",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flashback2k14/YeahTimeTrackingApi/issues"
  },
  "homepage": "https://github.com/flashback2k14/YeahTimeTrackingApi#readme",
  "main": "src/index.ts",
  "scripts": {
    "1-INIT-TASKS": "echo '#############################################'",
    "init": "npm run init:dirs && npm run generate:docs && npm run prisma:ts:to:db",
    "init:with:data": "npm run init && npm run prisma:seed",
    "init:dirs": "node scripts/init.js",
    "clean:all": "rm -rf dist/ db/ static/ coverage/",
    "format:all": "prettier --write .",
    "generate:docs": "node scripts/swagger.js",
    "2-DEV-TASKS": "echo '#############################################'",
    "start:dev": "DB_ENV=dev npm run prisma:reset && concurrently 'npm:tsc-watch' 'npm:node-watch'",
    "tsc-watch": "npx tsc --watch",
    "node-watch": "NODE_ENV=development dotenv -e .env.dev nodemon -q src/index.ts",
    "3-PROD-TASKS": "echo '#############################################'",
    "build": "npx tsc",
    "start": "NODE_ENV=production dotenv -e .env.prod node dist/src/index.js",
    "start:prod:with:build": "npm run build && npm start",
    "4-TESTING-TASKS": "echo '#############################################'",
    "pretest": "DB_ENV=test npm run prisma:dev",
    "test": "NODE_ENV=testing dotenv -e .env.test jest",
    "posttest": "rm -rf db/test/",
    "test:cov": "NODE_ENV=testing dotenv -e .env.test jest --coverage",
    "5-DB-TASKS": "echo '#############################################'",
    "prisma:db:to:ts": "DB_ENV=${DB_ENV:-dev}; dotenv -e .env.$DB_ENV prisma db pull",
    "prisma:ts:to:db": "DB_ENV=${DB_ENV:-dev}; dotenv -e .env.$DB_ENV prisma db push",
    "prisma:seed": "DB_ENV=${DB_ENV:-dev}; dotenv -e .env.$DB_ENV prisma db seed",
    "prisma:reset": "DB_ENV=${DB_ENV:-dev}; dotenv -e .env.$DB_ENV prisma migrate reset --force",
    "prisma:dev": "DB_ENV=${DB_ENV:-dev}; dotenv -e .env.$DB_ENV prisma migrate dev",
    "prisma:ui": "DB_ENV=${DB_ENV:-dev}; dotenv -e .env.$DB_ENV prisma studio",
    "prisma": "DB_ENV=${DB_ENV:-dev}; dotenv -e .env.$DB_ENV prisma"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "swagger-ui-express": "^4.6.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.14.5",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^7.6.0",
    "dotenv-cli": "^7.0.0",
    "jest": "^29.4.3",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "prisma": "^4.11.0",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}